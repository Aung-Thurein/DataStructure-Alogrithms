package com.turing.dsa.adt.Graph;

import org.junit.jupiter.api.Test;

import com.turing.dsa.adt.graph.Graph;

public class GraphTest {

	@Test
	public void testDFSBaseCase()
	{
		Graph graph = new Graph();
		
	    //A   -> B
        //B->C
        //C->D    
		graph.addVertex("A");
		graph.addVertex("B");
		graph.addVertex("C");
		graph.addVertex("D");
		graph.addVertex("E");
		
		graph.createAdjacencyMatrix();
	}
}
