package com.turing.dsa.adt.heap;

public class PriorityQueueWithHeap {

	Heap heap;
	
	public PriorityQueueWithHeap()
	{
		Integer[] empty = {};
		heap = new Heap(empty);
	}
	
	public void enqeue(int value)
	{
		this.heap.insert(value);
		this.heap.heapfiy();
		this.heap.buildTree();
	}
	
	public int deqeue()
	{
		return this.heap.deqeue();
	}
}

